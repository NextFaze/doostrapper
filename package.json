{
  "name": "doostrapper",
  "version": "0.1.7",
  "description": "CDK App Deployment boostrapper",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/whimzyLive/doostrapper"
  },
  "homepage": "https://github.com/whimzyLive/doostrapper/blob/master/README.md",
  "keywords": [
    "aws-cdk",
    "continuous-deployment",
    "continuous-delivery",
    "boostrapper"
  ],
  "devDependencies": {
    "@aws-cdk/assert": "^1.19.0",
    "@bazel/bazel": "2.0.0",
    "@bazel/buildifier": "0.29.0",
    "@bazel/ibazel": "0.11.1",
    "@bazel/jasmine": "^1.0.0",
    "@bazel/rollup": "^1.0.0",
    "@bazel/typescript": "^1.0.0",
    "@types/jasmine": "^3.5.0",
    "@types/node": "12.12.21",
    "jasmine": "^3.5.0",
    "prettier": "^1.19.1",
    "rollup": "^1.27.13",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "~3.7.4"
  },
  "dependencies": {
    "@aws-cdk/aws-cloudtrail": "^1.19.0",
    "@aws-cdk/aws-codebuild": "^1.19.0",
    "@aws-cdk/aws-codepipeline": "^1.19.0",
    "@aws-cdk/aws-codepipeline-actions": "^1.19.0",
    "@aws-cdk/aws-events": "^1.19.0",
    "@aws-cdk/aws-events-targets": "^1.19.0",
    "@aws-cdk/aws-iam": "^1.19.0",
    "@aws-cdk/aws-s3": "^1.19.0",
    "@aws-cdk/aws-sns": "^1.19.0",
    "@aws-cdk/aws-sns-subscriptions": "^1.19.0",
    "@aws-cdk/aws-ssm": "^1.19.0",
    "@aws-cdk/core": "^1.19.0",
    "change-case": "^4.1.1"
  },
  "scripts": {
    "bazel": "bazel",
    "build": "bazel build //...",
    "build:release": "./tools/release/publish.sh",
    "test": "bazel test //...",
    "pack": "bazel run :doostrapper.pack",
    "publish": "bazel run :doostrapper.publish",
    "build:watch": "ibazel build //...",
    "test:watch": "ibazel test //...",
    "format:check": "prettier --check ./**/*.ts",
    "format:write": "prettier --write ./**/*.ts",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "lint": "tslint --project ./",
    "clean": "bazel clean",
    "clean:deep": "bazel clean --expunge"
